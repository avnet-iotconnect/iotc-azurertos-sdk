{
   "updateId":{
      "provider":"@@@FW_PROVIDER@@@",
      "name":"@@@FW_NAME@@@",
      "version":"@@@FW_VERSION@@@"
   },
   "isDeployable":true,
   "compatibility":[
      {
         "deviceModel":"@@@DEVICE_MODEL@@@",
         "deviceManufacturer":"@@@DEVICE_MANUFACTURER@@@"
      }
   ],
   "instructions":{
      "steps":[
         {
            "type":"inline",
            "handler":"microsoft/swupdate:1",
            "files":[
               "@@@FW_FILENAME@@@"
            ],
            "handlerProperties":{
               "installedCriteria":"@@@DEVICE_MANUFACTURER@@@-@@@DEVICE_MODEL@@@-@@@FW_VERSION@@@"
            }
         }
      ]
   },
   "files":[
      {
         "filename":"@@@FW_FILENAME@@@",
         "sizeInBytes":@@@FW_FILE_SIZE@@@,
         "hashes":{
            "sha256":"@@@FW_SHA256@@@"
         }
      }
   ],
   "createdDateTime":"@@@CREATED_DATE@@@",
   "manifestVersion":"4.0"
}